{
  "version": 2,
  "builds": [
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/cassamedicalclinic"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/index.js"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/dist/cassamedicalclinic/index.html"
    }
  ]
}
